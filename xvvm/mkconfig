MKPATH = ../mk
<$MKPATH/inc/path
<$MKPATH/inc/util
<$MKPATH/inc/inclib
NAME = xvvm
SCRIPT = xde
VERSION = 6.2
CPPFLAGS = $CPPFLAGS $XINCPP
INC = $X11INC $FTINC
LIB = $LIB $X11LIB $FTLIB $XFTLIB
INC = $INC $XININC
LIB = $LIB $XINLIB
CPPFLAGS = $CPPFLAGS $INC\
	-DNAME=\"$NAME\"\
	-DVERSION=\"$VERSION\"\
	-D_DEFAULT_SOURCE\
	-D_BSD_SOURCE\
	-D_POSIX_C_SOURCE=200809L
CFLAGS = $CPPFLAGS
LDFLAGS = $LIB

