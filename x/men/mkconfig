<$ROOT/mkconfig
NAME = xmen
VERSION = 4.9
INC = $X11INC $FTINC 
LIB = $X11LIB $XFTLIB $FCLIB
INC = $INC $XININC
LIB = $LIB $XINLIB
CPPFLAGS = $INC\
	-DVERSION=\"$VERSION\"\
	-D_DEFAULT_SOURCE\
	-D_BSD_SOURCE\
	-D_XOPEN_SOURCE=700\
	-D_POSIX_C_SOURCE=200809L
CFLAGS = $CPPFLAGS
LDFLAGS = $LD_STATIC_FLAG $LIB

